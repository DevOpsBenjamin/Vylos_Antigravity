    },
    "devDependencies": {
        "@types/node": "^22.9.3",
        "@types/fs-extra": "^11.0.4",
        "typescript": "^5.6.3",
        "tailwindcss": "^3.4.15",
        "postcss": "^8.4.49",
        "autoprefixer": "^10.4.20",
        "vue-tsc": "^2.1.10",
